#include<iostream>
using namespace std;
int main()
{
	int n;
	cout<<"Enter number of Rows :";
	cin>>n;
	for(int i=1;i<=n;i++)
	{
		for(int s=1;s<=n-i;s++)
			{
				cout<<" ";
			}
		for(int j=i;j<=2*i-1;j++)
			{
				cout<<j;
			}		
		for(int k=1;k<i;k++)
			{
				cout<<2*i-k-1;
			}	

		cout<<endl;
	}
	return 0;
}
